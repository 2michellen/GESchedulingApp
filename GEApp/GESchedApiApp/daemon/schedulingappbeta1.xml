<service>
	<id>schedulingappbeta1.exe</id>
	<name>SchedulingAppBeta1</name>
	<description>The Scheduling App.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>--max_old_space_size=4096</argument>
	<argument>C:\Users\jacknip\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\GESchedulingApp\GESchedulingApp\GEApp\GESchedApiApp\server.js</argument>
	<argument>--log</argument>
	<argument>SchedulingAppBeta1 wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
